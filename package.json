{
  "name": "sheetmetal.io",
  "version": "00.00.02",
  "description": "Turn a Google Sheet into an API",
  "main": "./server",
  "scripts": {
    "build": "NODE_ENV=production next build",
    "deploy": "NODE_ENV=production next build && gcloud app deploy --quiet --project=sheetmetal",
    "dev": "node server",
    "migrate": "node-pg-migrate -m ./db/migrations -f ./db/config.js",
    "migrate:prod": "NODE_ENV=production node ./db/migrateProd.js",
    "pretty": "prettier --write \"./**/*.{js,json,yml,md,vue,css,scss}\"",
    "start": "NODE_ENV=production node server",
    "test": "jest"
  },
  "dependencies": {
    "@next/mdx": "^8.0.4",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.1",
    "bulma-extensions": "^6.2.4",
    "cookie-parser": "^1.4.4",
    "custom-env": "^1.0.2",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
    "googleapis": "^38.0.0",
    "install": "^0.12.2",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.0",
    "next": "^8.1.1-canary.55",
    "node-sass": "^4.11.0",
    "pg": "^7.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-toastify": "^4.5.2",
    "sentry-testkit": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@mdx-js/loader": "^0.20.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "husky": "^1.2.1",
    "jest": "^24.8.0",
    "node-pg-migrate": "^3.18.0",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "webpack": "^4.35.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
